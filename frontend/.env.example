# GreenPort Frontend Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api

# Feature Flags (optional)
VITE_ENABLE_DEMO_MODE=true
VITE_ENABLE_ANALYTICS=false

# Website Carbon API (optional - for real carbon tracking)
# Get your API key at: https://www.websitecarbon.com/api/
VITE_CARBON_API_KEY=your_api_key_here
